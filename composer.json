{
  "name": "jaai/hotelavail-api",
  "description": "Bank api",
  "license": "proprietary",
  "type": "project",
  "version": "1.0.0",
  "require": {
    "php": ">=7.0.0",
    "laravel/lumen-framework": "5.6.*",
    "jenssegers/mongodb": "^3.3",
    "vlucas/phpdotenv": "~2",
    "predis/predis": "~1",
    "illuminate/redis": "5.6.*",
    "illuminate/bus": "^5.6"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "cache-dir": "./../composer-cache",
    "bin-dir": "./bin"
  },
  "require-dev": {
    "scrutinizer/ocular": "~1.1",
    "phpunit/phpunit": "~7",
    "codeception/codeception": "~2",
    "codeception/aspect-mock": "~3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "database/"
    ]
  },
  "extra": {
    "writable": [
      "storage"
    ]
  },
  "repositories": [
  ],
  "scripts": {
  }
}
